package GenericsArray;

public class GenericsArray1<T> {
    private T[] array;

    public GenericsArray1() {
    }

    public GenericsArray1(T[] array) {
        this.array = array;
    }

    public T[] getArray() {
        return array;
    }

    public void setArray(T[] array) {
        this.array = array;
    }

    public T getValueByIndex(int i) {
        return array[i];
    }

}
