package GenericsArray;

public class GenericsArray1<T> {
    private T[] array;  //creating an array of T-type

    public GenericsArray1() {
    }

    public GenericsArray1(T[] array) {  //a constructer with an argument
        this.array = array;
    }

    public T[] getArray() {
        return array;
    }

    public void setArray(T[] array) {
        this.array = array;
    }

    public T getValueByIndex(int i) {
        return array[i];
    }

}
